[
    {% for i in identifier -%}
    {
        "use": "{{ i.use }}",
        "type":
        {
            "coding":
            [
                {% for c in i.type.coding -%}
                {
                    "system": "{{ c.system }}",
                    "version": "{{ c.version }}",
                    "code": "{{ c.code }}",
                    "display": "{{ c.display }}",
                    {% if c.userSelected -%}
                    "userSelected": {{ c.userSelected }},
                    {% endif -%}
                },
                {% endfor -%}
            ],
            "text": "{{ i.type.text | redact }}"
        },
        "system": "{{ i.system }}",
        "value": "{{ i.value | redact }}",
        "period":
        {
            "start": "{{ i.period.start | date_shift }}",
            "end": "{{ i.period.end | date_shift }}",
        },
        "assigner":
        {
            "reference": "{{ i.reference }}",
            "type": "{{ i.uri }}",
            "display": "{{ i.display | redact }}"
        }
    },
    {% endfor -%}
],