"use":{% include 'ValueSet/NameUse' code: HumanName.use -%},
"family":"{{HumanName.family._}}",
"given":[
	{% assign givens = HumanName.given | to_array -%}
	{% for g in givens -%}
		{% if g._ -%}
			"{{g._}}",
		{% elsif g -%}
			"{{g}}",
		{% endif -%}
	{% endfor -%}
],
"prefix":
[
	"{{HumanName.prefix._}}",
],
"suffix":
[
	"{{HumanName.suffix._}}",
]