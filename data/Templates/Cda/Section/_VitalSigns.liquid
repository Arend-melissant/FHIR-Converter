{% assign firstSections = msg | get_first_cda_sections_by_template_id: '2.16.840.1.113883.10.20.22.2.4.1' -%}
{% assign entries = firstSections.2_16_840_1_113883_10_20_22_2_4_1.entry | to_array -%}
{% for e in entries -%}
    {% assign components = e.organizer.component | to_array -%}
    {% for c in components -%}
        {% assign observationId = c.observation | to_json_string | generate_uuid -%}
        {% include 'Resource/Observation' observationCategory: 'vital-signs', observationEntry: c.observation, ID: observationId -%}
    {% endfor -%}
{% endfor -%}