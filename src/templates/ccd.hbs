{
    "resourceType": "Bundle",
    "type": "transaction",
    "entry": [
        {{#with msg.ClinicalDocument.recordTarget.patientRole}}
            {{>Resources/Patient.hbs patientRole=this ID=(generateUUID (toJsonString this.id))}},
            {{>Resources/Organization.hbs org=this.providerOrganization ID=(generateUUID (toJsonString this.providerOrganization.id))}},
            {{>References/Patient/managingOrganization.hbs ID=(generateUUID (toJsonString this.id)) REF=(concat 'Organization/' (generateUUID (toJsonString this.providerOrganization.id)))}},
          {{/with}}
       
        {{#with (getFirstCdaSections msg 'Allergies')}}
            {{#each Allergies.entry as |alEntry|}}
                {{>Resources/AllergyIntolerance.hbs allergyEntry=alEntry ID=(generateUUID (toJsonString alEntry.act.id))}},
                {{>References/AllergyIntolerance/patient.hbs ID=(generateUUID (toJsonString alEntry.act.id)) REF=(concat 'Patient/' (generateUUID (toJsonString ../../msg.ClinicalDocument.recordTarget.patientRole.id)))}},
            {{/each}}
        {{/with}},
      ]
}