{
    "fullUrl":"urn:uuid:{{ID}}",
    "resource":{
        "resourceType": "CarePlan",
        "id":"{{ID}}",
        
        "status":"{{>ValueSet/RequestStatus.hbs code=this.statusCode.code}}",

        "code":
        	[
            	{{#if careplanEntry.code.translation}},
                    {{>DataType/CodeableConcept.hbs code=careplanEntry.code.translation}},
                {{else}},
                    {{>DataType/CodeableConcept.hbs code=careplanEntry.code}},
                {{/if}}
            ],   
        
        "effectivePeriod":
        {
            "start":"{{formatAsDateTime careplanEntry.effectiveTime.low.value}}",
            "end":"{{formatAsDateTime careplanEntry.effectiveTime.high.value}}",
        }
        
        "effectiveDateTime":"{{formatAsDateTime careplanEntry.effectiveTime.center.value}}",
    },
    "request":{
        "method":"POST",
        "url":"CarePlan",
    },
},
