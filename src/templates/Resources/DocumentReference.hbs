{
    "fullUrl":"urn:uuid:{{ID}}",
    "resource":{
        "resourceType": "DocumentReference",
        "id":"{{ID}}",
        "type":
		[
        	{{#each (toArray docref.ClinicalDocument.code)}}
        		{{>DataType/CodeableConcept.hbs code=this}},
           	{{/each}}
        ],
        "content":
        [
            {
                "attachment":
                {
                    "contentType":"text/plain",
                    "data":"{{{base64Encode docref._originalData}}}",
                    "hash":"{{{base64Encode (sha1Hash docref._originalData)}}}",
                }
            }
        ]
        "indexed":"{{formatAsDateTime docref.ClinicalDocument.effectiveTime.value}}"
    },
    "request":{
        "method":"POST",
        "url":"DocumentReference",
    },
},