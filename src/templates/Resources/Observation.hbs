{
    "fullUrl":"urn:uuid:{{ID}}",
    "resource":{
        "resourceType": "Observation",
        "id":"{{ID}}",
        "code":
        	{
            "coding":
        		[
            	{{>DataType/Coding.hbs code=observationEntry.code}},

                {{>DataType/Coding.hbs code=observationEntry.code.translation}},

        ],
        }
        "statusCode":"{{observationEntry.statusCode.code}}"
        "effectivePeriod":
        	{"start":"{{formatAsDateTime observationEntry.effectiveTime.value}}"
            "start":"{{formatAsDateTime observationEntry.effectiveTime.low.value}}"}
        "valueQuantity":
        {
        	{{#each (toArray observationEntry.value)}}
            	"value":"{{this.value}}",
                "unit":"{{this.unit}}",
  			{{/each}}
        },
        
    },
    "request":{
        "method":"POST",
        "url":"Observation",
    },
},
