{
    "fullUrl":"urn:uuid:{{ID}}",
    "resource":{
        "resourceType": "Observation",
        "id":"{{ID}}",
        "code":
        [
        	{{#each (toArray observationEntry.code)}}
            	{{>DataType/CodeableConcept.hbs code=this}},
                [
                {{#each (toArray this.translation)}}
                	{{>DataType/Coding.hbs code=this}},
                {{/each}}
                ]
  			{{/each}}
        ],
        "statusCode":"{{observationEntry.statusCode.code}}"
        "effectivePeriod":
        	{"start":"{{formatAsDateTime observationEntry.effectiveTime.value}}"
            "start":"{{formatAsDateTime observationEntry.effectiveTime.low.value}}"}
        "valueQuantity":
        {
        	{{#each (toArray observationEntry.value)}}
            	"value":"{{this.value}}",
                "unit":"{{this.unit}}",
  			{{/each}}
        },
        
    },
    "request":{
        "method":"POST",
        "url":"Observation",
    },
},
