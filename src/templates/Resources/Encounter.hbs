{
    "fullUrl":"urn:uuid:{{ID}}",
    "resource":{
        "resourceType": "Encounter",
        "id":"{{ID}}",
        "status":"{{encounter.statusCode}}",
        "class":{{>DataType/Coding.hbs code=encounter.code.translation}},
        "type":
        [
           	{{>DataType/CodeableConcept.hbs code=encounter.code}},
        ],
        "identifier":
        [
        	{{#each (toArray encounter.id)}}
        		{{>DataType/Identifier.hbs id=this}},
            {{/each}}
        ],
        "period": {{>DataType/Period.hbs period=encounter.effectiveTime}},
        "priority":{{>DataType/CodeableConcept.hbs code=encounter.priorityCode}},
       
        "hospitalization":
            {
            "dischargeDisposition":{{>DataType/CodeableConcept.hbs code=encounter.sdtc:dischargeDispositionCode}},
            }
     },
    "request":{
        "method":"POST",
        "url":"Encounter",
    },
},
