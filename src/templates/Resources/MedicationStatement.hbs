{
    "fullUrl":"urn:uuid:{{ID}}",
    "resource":{
        "resourceType": "MedicationStatement",
        "id":"{{ID}}",
        "identifier":
  		[
        	{{#each (toArray medicationEntry.substanceAdministration.id)}}
            	{{>DataType/Identifier.hbs id=this}}
            {{/each}}
        ],
        "status":"{{medicationEntry.substanceAdministration.statusCode.code}}",
        "effectivePeriod":
        	{"start":"{{formatAsDateTime medicationEntry.substanceAdministration.effectiveTime.low.value}}",
            "end":"{{formatAsDateTime medicationEntry.substanceAdministration.effectiveTime.high.value}}"
        	},
        "taken":
    },
    "request":{
        "method":"POST",
        "url":"MedicationStatement",
    },
},
