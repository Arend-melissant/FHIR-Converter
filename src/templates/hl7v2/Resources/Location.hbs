{
    "fullUrl":"urn:uuid:{{ID}}",
    "resource":{
        "resourceType": "Location",
        "id":"{{ID}}",
        {{>DataType/PL.hbs PL=ROL-13}},
        {{>DataType/PL.hbs PL=PV1-3}},
        {{>DataType/PL.hbs PL=PV1-6}},
        {{#each (getFieldRepeats PR1-23)}}
            {{>DataType/PL.hbs PL=this}},
        {{/each}}
        {{#each (getFieldRepeats PRT-9)}}
            {{>DataType/PL.hbs PL=this}},
        {{/each}}
        {{#each (getFieldRepeats ORC-21)}}
            {{>DataType/XONLocation.hbs XON=this}},
        {{/each}}
        "address":
        {
            {{#each (getFieldRepeats ORC-22)}}
                {{>DataType/XAD.hbs XAD=this}},
            {{/each}}
            {{>DataType/XAD.hbs XAD=RXA-28}},
        },
        "telecom":
        [
            {{#each (getFieldRepeats ORC-23)}}
                { {{>DataType/XTN.hbs XTN=this}} },
            {{/each}}
        ],
        {{>DataType/PL.hbs PL=RXA-27}},
    },
    "request":{
        "method":"PUT",
        "url":"Location/{{ID}}",
    },
},
