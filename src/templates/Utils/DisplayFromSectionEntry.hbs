{{#with (evaluate 'Utils/ResourceTypeFromSection.hbs' section=section)}}
    {{#if (eq this.resourceType 'AllergyIntolerance')}}
        {{#with (evaluate 'Resources/AllergyIntolerance.hbs' allergyEntry=../entry) as |obj|}}
           	"display":"{{obj.resource.reaction.[0].substance.coding.[0].display}}"
        {{/with}}
    {{/if}}
    
	{{#if (eq this.resourceType 'Observation')}}
    "x":"{{toJsonString ../entry.observation.code.originalText}}",
        {{#with (evaluate 'Resources/Observation.hbs' observationEntry=../entry.observation) as |obj|}}
           	"display":"{{obj.resource.code.[0].coding.[0].display}}"
        {{/with}}
    {{/if}}
    
{{/with}}



