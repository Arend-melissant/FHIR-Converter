{{#with (getFirstCdaSectionsWithExactMatch msg 'Functional Status')}}

    {{#each (toArray Functional_Status.entry) as |funcEntry|}}
    
        {{#if (eq funcEntry.observation.templateId.root '2.16.840.1.113883.10.20.22.4.4')}}
            {{>Resources/Condition.hbs conditionEntry=funcEntry.observation ID=(generateUUID (toJsonString funcEntry.observation))}},
            {{>References/Condition/subject.hbs ID=(generateUUID (toJsonString funcEntry.observation)) REF=(concat 'Patient/' (generateUUID (toJsonString ../../msg.ClinicalDocument.recordTarget.patientRole)))}},
        {{/if}}
        
        {{#if (eq funcEntry.observation.templateId.root '2.16.840.1.113883.10.20.22.4.2')}}
            {{>Resources/Observation.hbs observationEntry=funcEntry.observation ID=(generateUUID (toJsonString funcEntry.observation))}},
            {{>References/Observation/subject.hbs ID=(generateUUID (toJsonString funcEntry.observation)) REF=(concat 'Patient/' (generateUUID (toJsonString ../../msg.ClinicalDocument.recordTarget.patientRole)))}},
        {{/if}}
        
    {{/each}}
    
{{/with}} 