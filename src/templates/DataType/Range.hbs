        {{else}}
            "value":{{range.low.value}},
        {{/if}}
        "unit":"{{range.low.unit}}",
    }
    "high":
    {
        {{#if (and range.high.value (eq (length (match range.high.value '[0123456789+-eE.]+'))))}}
            "value":"{{range.high.value}}",
        {{else}}
            "value":{{range.high.value}},
        {{/if}}
        "unit":"{{range.high.unit}}",
    }
