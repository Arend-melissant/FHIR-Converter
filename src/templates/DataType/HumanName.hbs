{
	"use":"{{name.use}}",
	"family":"{{name.family}}",
	"given":[
       	{{#each (toArray name.given)}}
           	{{#if (isString this)}}
               	"{{this}}",
           	{{else}}
               	"{{this._}}",
           	{{/if}}
       	{{/each}}
    ]
	"prefix":"{{name.prefix}}",
    "suffix":"{{name.suffix}}",
}
